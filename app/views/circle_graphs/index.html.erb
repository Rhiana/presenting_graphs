<p id="notice"><%= notice %></p>

<h1>Circle Graphs</h1>

<table>
  <thead>
    <tr>
      <th>Score</th>
      <th>Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @circle_graphs.each do |circle_graph| %>
      <tr>
        <td><%= circle_graph.score %></td>
        <td><%= circle_graph.total %></td>
        <td><%= link_to 'Show', circle_graph %></td>
        <td><%= link_to 'Edit', edit_circle_graph_path(circle_graph) %></td>
        <td><%= link_to 'Destroy', circle_graph, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Circle Graph', new_circle_graph_path %>
